package InterviewQuations;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class MainClass {

	public static void main(String[] args) {

		
		Employee em = new Employee(1, "Tushar", "tmakude@gmail.com");
		Employee em1 = new Employee(2, "Onkar", "onkarChaudhari@gmail.com");
		Employee em2 = new Employee( 2, "Onkar", "onkarChaudhari@gmail.com");
		Employee em3 = new Employee(5, "gaurav", "ngav@gmail.com");
		
		Map<Employee ,String> map = new HashMap<>();
		
		map.put(em, "Employee 1");
		map.put(em1, "Employee 2");
		map.put(em2, "Employee 3");
		map.put(em3, "Employee 4");
		
		map.forEach((a , b)-> System.out.println( a.getId() + ","+ b));
		
		



	}

}
